[ghost_dev]
server1 ansible_host=3.93.76.128 blog_url=http://ec2-3-93-76-128.compute-1.amazonaws.com

# How to add another environment
# [ghost_prod]
# server2 ansible_host=3.93.76.129 blog_hostname=ec2-3-93-76-129.compute-1.amazonaws.com

[ghost_dev:vars]
ansible_ssh_user=ubuntu
ansible_ssh_private_key_file=~/.ssh/ghost-admin.pem
blog_mail_from=mail@example.com
blog_mail_username=username
blog_mail_pass=userpassword
